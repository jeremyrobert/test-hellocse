{
    "openapi": "3.0.0",
    "info": {
        "title": "Test Technique - HelloCSE",
        "description": "API Documentation",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000",
            "description": "Test Technique v1"
        }
    ],
    "paths": {
        "/api/auth/register": {
            "post": {
                "tags": [
                    "Authentification"
                ],
                "summary": "Register a new administrator",
                "description": "Register a new administrator and return an access token and a refresh token.",
                "operationId": "d43ecdfd17aca209ecd9b0a8b370e2c0",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "$ref": "#/components/schemas/RegisterRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Successful registration"
                    },
                    "422": {
                        "description": "Unprocessable Content"
                    }
                }
            }
        },
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Authentification"
                ],
                "summary": "Log in as administrator",
                "description": "Log in an administrator and return an access token and a refresh token.",
                "operationId": "be2e78e5f93182d4403f839cc516d312",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful login"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "422": {
                        "description": "Unprocessable Content"
                    },
                    "429": {
                        "description": "Too Many Requests"
                    }
                }
            }
        },
        "/api/auth/refresh-token": {
            "get": {
                "tags": [
                    "Authentification"
                ],
                "summary": "Refresh the access token",
                "description": "Refresh and return the access token.",
                "operationId": "58f1e2cba1b045b98578442e6260becf",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "422": {
                        "description": "Unprocessable Content"
                    }
                },
                "security": [
                    {
                        "refresh_token": []
                    }
                ]
            }
        },
        "/api/v1/profile": {
            "post": {
                "tags": [
                    "Profile"
                ],
                "summary": "Store a new profile.",
                "description": "Store a new profile with the given data.",
                "operationId": "d7071a56393c5fd063728deebf0f30d2",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "$ref": "#/components/schemas/StoreProfileRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful login"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "422": {
                        "description": "Unprocessable Content"
                    },
                    "429": {
                        "description": "Too Many Requests"
                    }
                },
                "security": [
                    {
                        "access_token": []
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "LoginRequest": {
                "title": "Login Request",
                "description": "Request structure for administrator login",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "description": "The email address of the administrator",
                        "type": "string",
                        "format": "email",
                        "example": "user@example.com"
                    },
                    "password": {
                        "description": "The password for the administrator",
                        "type": "string",
                        "format": "password",
                        "example": "password"
                    }
                },
                "type": "object"
            },
            "RegisterRequest": {
                "title": "Register Request",
                "description": "Request structure for administrator register",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "description": "The email address of the administrator",
                        "type": "string",
                        "format": "email",
                        "example": "user@example.com"
                    },
                    "password": {
                        "description": "The password for the administrator",
                        "type": "string",
                        "format": "password",
                        "example": "password"
                    }
                },
                "type": "object"
            },
            "StoreProfileRequest": {
                "title": "Store Profile Request",
                "description": "Request structure for storing a new profile.",
                "required": [
                    "last_name",
                    "first_name",
                    "image",
                    "status"
                ],
                "properties": {
                    "last_name": {
                        "description": "The last name of the profile",
                        "type": "string",
                        "example": "Doe"
                    },
                    "first_name": {
                        "description": "The first name of the profile",
                        "type": "string",
                        "example": "John"
                    },
                    "image": {
                        "description": "The image of the profile",
                        "type": "string",
                        "format": "base64"
                    },
                    "status": {
                        "description": "The status of the profile",
                        "type": "string",
                        "enum": [
                            "inactive",
                            "pending",
                            "active"
                        ]
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "access_token": {
                "type": "apiKey",
                "description": "Enter token in format (Bearer {token})",
                "name": "Authorization",
                "in": "header"
            },
            "refresh_token": {
                "type": "apiKey",
                "description": "Enter token in format (Bearer {token})",
                "name": "Authorization",
                "in": "header"
            }
        }
    },
    "tags": [
        {
            "name": "Authentification",
            "description": "Group of endpoints for authentification."
        },
        {
            "name": "Profile",
            "description": "Group of endpoints for managing profiles."
        }
    ]
}